{
    "name": "eaudeweb/recommended-project",
    "description": "Project template for Drupal 11 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/password_strength",
                 "drupal/layout_builder_st",
                 "drupal/node_access_grants"
            ]
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/password_strength",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/password_strength-3329409.git",
                    "reference": "3329409-automated-drupal-10"
                },
                "require": {
                    "drupal/password_policy": "^4.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/layout_builder_st",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/layout_builder_st-3497945.git",
                    "reference": "3497945-minimal-phpcs"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "slick/slick",
                "version": "v1.9.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/master.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_blocks.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_decoupled.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_demo_data.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_event.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_group.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_media.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_paragraphs.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_person.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_project.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_utilities.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_themes.git"
        },
        {
            "type": "git",
            "url": "https://github.com/eaudeweb/edw_document.git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://git.drupalcode.org/issue/node_access_grants-3494785"
        }
    ],
    "require": {
        "php": "^8.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/adequate_passwords": "^1.0",
        "drupal/admin_toolbar": "^3.6",
        "drupal/ajax_loader": "^2.1",
        "drupal/allowed_formats": "^3.0",
        "drupal/autologout": "^1.6",
        "drupal/backup_migrate": "^5.1",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/blazy": "^3.0",
        "drupal/block_field": "^1.0@RC",
        "drupal/bootstrap_layout_builder": "^2.0",
        "drupal/captcha": "^2.0",
        "drupal/ckeditor5_paste_filter": "^1.1",
        "drupal/ckeditor_accordion": "^2.3",
        "drupal/ckeditor_bs_grid": "^2.0",
        "drupal/clamav": "^2.1",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "^11",
        "drupal/core-project-message": "^11",
        "drupal/core-recommended": "^11",
        "drupal/crop": "^2.4",
        "drupal/csv_serialization": "^4.0",
        "drupal/diff": "^1.9",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/edw_healthcheck": "^1.27",
        "drupal/edwt": "^4.1@beta",
        "drupal/elogger": "^1.1",
        "drupal/encrypt": "^3.2",
        "drupal/entity_clone": "^2.1@beta",
        "drupal/entity_embed": "^1.6",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^2.0",
        "drupal/facets": "^3.0",
        "drupal/field_group": "^4.0",
        "drupal/gin": "^5.0",
        "drupal/honeypot": "^2.2",
        "drupal/image_widget_crop": "^3.0",
        "drupal/imce": "^3.1",
        "drupal/lang_dropdown": "^2.3",
        "drupal/layout_builder_browser": "^1.8",
        "drupal/layout_builder_modal": "^1.3",
        "drupal/layout_builder_restrictions": "^3.0",
        "drupal/layout_builder_st": "dev-custom",
        "drupal/layout_paragraphs": "^2.1",
        "drupal/link_attributes": "^2.1",
        "drupal/masquerade": "^2.0",
        "drupal/media_library_edit": "^3.0",
        "drupal/media_library_form_element": "^2.1",
        "drupal/media_responsive_thumbnail": "^2.0",
        "drupal/menu_link_attributes": "^1.5",
        "drupal/metatag": "^2.1",
        "drupal/oembed_lazyload": "^2.0",
        "drupal/optional_end_date": "^1.4",
        "drupal/paragraphs": "^1.19",
        "drupal/pathauto": "^1.13",
        "drupal/protected_pages": "^1.7",
        "drupal/real_aes": "^2.5",
        "drupal/recaptcha": "^3.4",
        "drupal/redirect": "^1.11",
        "drupal/remove_username": "^1.3",
        "drupal/sdc_display": "^2.0@beta",
        "drupal/search_api": "^1.38",
        "drupal/select2": "^2.0",
        "drupal/simple_search_form": "^1.6",
        "drupal/slick": "^3.0",
        "drupal/slick_views": "^3.0",
        "drupal/smart_trim": "^2.2",
        "drupal/smtp": "^1.4",
        "drupal/spamspan": "^3.2",
        "drupal/svg_image": "^3.1",
        "drupal/tfa": "^2.0@alpha",
        "drupal/token": "^1.15",
        "drupal/twig_tweak": "^3.4",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_data_export": "^1.5",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.3@beta",
        "drupal/webp": "^1.0@beta",
        "drupal/xmlsitemap": "^2.0",
        "drush/drush": "^13",
        "eaudeweb/edw_blocks": "^1.0",
        "eaudeweb/edw_decoupled": "^1.0",
        "eaudeweb/edw_demo_data": "^1.0",
        "eaudeweb/edw_document": "^1.0",
        "eaudeweb/edw_event": "^1.0",
        "eaudeweb/edw_group": "^1.0",
        "eaudeweb/edw_media": "^1.0",
        "eaudeweb/edw_paragraphs": "^1.0",
        "eaudeweb/edw_person": "^1.0",
        "eaudeweb/edw_project": "^1.0",
        "eaudeweb/edw_themes": "^1.0",
        "eaudeweb/edw_utilities": "^1.0",
        "eaudeweb/heavy-lifter": "^2",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "slick/slick": "^1.9.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^3.0@alpha",
        "drupal/coder": "^8.3",
        "drupal/core-dev": "^11",
        "drupal/devel": "^5.1",
        "drupal/stage_file_proxy": "^3.1",
        "drupal/twig_vardumper": "^3.0",
        "mglaman/phpstan-drupal": "^1.1",
        "overtrue/phplint": "^9.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/example.settings.local.php": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "enable-patching": true,
        "patches-file": "composer.patches.json",
        "hooks": {
            "pre-commit": [
                "./code-qa.sh"
            ]
        }
    },
    "scripts": {
        "remove-git-folders": [
            "scripts/composer/remove-git-folders.sh"
        ],
        "post-install-cmd": [
            "@remove-git-folders",
            "bash -c 'vendor/bin/cghooks update > /dev/null 2>&1 || true'",
            "cd web/themes/contrib/edwt/ && npm install",
            "cd web/themes/custom/frontend/ && npm install && npm run build"
        ],
        "post-update-cmd": [
            "@remove-git-folders"
        ],
        "post-drupal-scaffold-cmd": [],
        "theme-build": [
            "cd web/themes/contrib/edwt/ && npm install",
            "cd web/themes/custom/frontend/ && npm install && npm run build"
        ],
        "theme-watch": [
            "Composer\\Config::disableProcessTimeout",
            "cd web/themes/contrib/edwt/ && npm install",
            "cd web/themes/custom/frontend/ && npm install && npm run watch"
        ]
    }
}
