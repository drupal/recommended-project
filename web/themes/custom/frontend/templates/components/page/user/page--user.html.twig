{#
/**
 * @file
 * EDWT override to display a single page.
 *
 * @see page.html.twig
 */
#}

{%
  set comp_classes = [
    'page--user',
  ]
%}

{%
  set main_classes = [
    'grid-container',
    'grid-container_sm',
  ]
%}

{% include "frontend:page-layout" with {
  uc: comp_classes,
  uc_main: main_classes
} %}
